@model Track

<a asp-controller="Home" asp-action="Track" asp-route-trackName="@Model.Name"
   asp-route-artistName="@Model.Artist.Name" class="panel panel-success">
    <div class="panel-heading">
        <p class="text-center">@Model.Name</p>
    </div>
    <div class="panel-body">
        <img class="img-rounded center-block" src="@Model.PictureLink" />
    </div>
    <div class="panel-footer">
        <p>
            <span class="glyphicon glyphicon-user"></span>    @Model.Artist.Name
        </p>
        <p>
            <span class="glyphicon glyphicon-headphones"></span>    @Model.Listeners
        </p>
    </div>
</a>

