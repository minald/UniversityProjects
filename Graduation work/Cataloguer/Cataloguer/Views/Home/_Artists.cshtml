@model IEnumerable<Artist>

@foreach (Artist artist in Model)
{
    <a asp-controller="Home" asp-action="Artist" asp-route-name="@artist.Name" class="panel panel-success">
        <div class="panel-heading">
            <p class="text-center">@artist.Name</p>
        </div>
        <div class="panel-body">
            <img class="img-rounded center-block" src="@artist.PictureLink" />
        </div>
    </a>
}
